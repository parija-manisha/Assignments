
@{
    ViewBag.Title = "UserList";
}

<h2>UserList</h2>
<table id="UserListTable" class="table table-striped table-bordered">
    <thead class="thead-dark">
        <tr>
            <th>Sl.No</th>
            <th data-sortExpression="Name">Name</th>
            <th data-sortExpression="Gender">Gender</th>
            <th data-sortExpression="Email">Email</th>
            <th data-sortExpression="PhoneNumber">Phone Number</th>
            <th data-sortExpression="DateOfBirth">DOB</th>
            <th data-sortExpression="FatherName">Father Name</th>
            <th data-sortExpression="MotherName">Mother Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody></tbody>
</table>


