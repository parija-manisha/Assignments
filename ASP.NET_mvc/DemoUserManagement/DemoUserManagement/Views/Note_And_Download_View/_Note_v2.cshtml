@{
    Layout = null;
}

@{ 
    var objectType = ViewData["objectType"] as string;
}


<div id="Note" class="container mt-5">
    <div class="row">
        <div>
            <div class="row">
                <input type="hidden" id="PageName" name="PageName" value="@ViewBag.PageName" />
                <textarea id="AddNote" class="form-control" rows="4" placeholder="Enter Your Note"></textarea>
            </div>
            <button id="AddNoteBtn" class="btn btn-primary mt-3" onclick="addNote(); return false;">Add</button>
            <label id="AddSuccess" class="text-success mt-2"></label>
        </div>
    </div>
    <br />
    <div>
        @{
            <table id="NoteListTable" class="table table-striped table-bordered">
                <thead class="thead-dark">
                    <tr>
                        <th>Sl.No</th>
                        <th>Object ID</th>
                        <th>Page Name ID</th>
                        <th>Note</th>
                        <th>Entered At</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        }
    </div>
</div>
